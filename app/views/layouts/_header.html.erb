<nav>

<% if current_user %>

	<%= link_to "Home", projects_path %>
	<%= link_to "New Project", new_project_path %>
	<%= link_to "Log Out", logout_path %>

	 <!--Displays current user's avatar and links to that user's index page-->
	<a href=<%= "#{user_path(current_user)}" %>><img src="<%= current_user.avatar_url %>"></a>

<% else %>
	<%= link_to "Home", projects_path %>
	<%= link_to "Log In", login_path %>
	<%= link_to "Sign Up", signup_path %>
<% end %>

</nav>
