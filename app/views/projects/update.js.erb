
var $button = $(document).find("input.project-complete-btn")
$("#recorderStart").toggle()
$("#instrument-need-checkbox").toggle()

if ($button.val() === "Project Complete"){
	$button.val("Allow Collaboration");
	$button.siblings().last().val("false");
}
else {
	$button.val("Project Complete");
	$button.siblings().last().val("true");
}

